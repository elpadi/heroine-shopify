#comments h3 {
	font-size:1.1em;
	margin-bottom:0;
}
#comments p {
	padding-left:1em;
}
#comments h2,
#comments-form h2 {
	font-size:1.4em;
}
#comments-form {
	max-width:350px;
}
#comments-form form {
	overflow:auto;
}
#comments-form label {
	display:none;
}
#comments-form .text {
	display:block;
	margin:4% 0;
	width:100%;
	height:32px;
	box-sizing:border-box;
}
#comments-form .notice {
	margin-top:2%;
	text-align:right;
}

#cart-animation {
	display:none !important;
}

#add_address h4 {
	display:none;
}
#add_address td {
	border-style:none;
}

#page .page-title {
	font-size:1.1em;
	margin-bottom:5%;
}
#page p {
	font-size:0.925em;
}
#page h2,
#page h3,
#page h4 {
	font-size:1.075em;
}

.template--page.press .page-content {
	overflow:auto;
	font-size:0.85em;
}
.template--page.press h2 {
	color:#808080;
}
.template--page.press #page:not(.page--press) h2 {
	margin:8px 0 0;
}
.template--page.press p {
	margin-bottom:17px;
}
@media (max-width: 767px) {
	.template--page.press p {
		float:none;
	}
}
.page--press h2 {
	clear:both;
}
.page--press a {
}
.page--press .page-content > div::after {
	content:'';
	display:block;
	clear:both;
}
.page--press .page-content > div > a {
	float:left;
	margin-bottom:25px;
	vertical-align:middle;
	width:27%;
	max-width:100px;
	margin-left:9.5%;
}
.page--press .page-content > div > a:nth-child(3n + 1) {
	clear:left;
	margin-left:0;
}
@media (min-width:440px) {
	.page--press .page-content > div > a {
		margin-left:50px;
	}
}
@media (min-width:590px) {
	.page--press .page-content > div > a:nth-child(3n + 1) {
		clear:none;
		margin-left:50px;
	}
	.page--press .page-content > div > a:nth-child(4n + 1) {
		clear:left;
		margin-left:0;
	}
}
@media (min-width:740px) {
	.page--press .page-content > div > a:nth-child(4n + 1) {
		clear:none;
		margin-left:50px;
	}
	.page--press .page-content > div > a:nth-child(5n + 1) {
		clear:left;
		margin-left:0;
	}
}
.page--press img {
	height:133px;
	width:100%;
	object-fit:cover;
	object-position:left top;
}
