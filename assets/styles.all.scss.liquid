.header__mobile-menu {
	opacity:0;
	visibility:hidden;
	-ms-transform:translateY(-100%);
	-webkit-transform:translateY(-100%);
	transform:translateY(-100%);
}
body.mobile-menu-visible .header__mobile-menu {
	opacity:1;
	visibility:visible;
	-ms-transform:translateY(0);
	-webkit-transform:translateY(0);
	transform:translateY(0);
}
body.mobile-menu-visible .flexslider-container,
body.mobile-menu-visible #slideshow-sale-tag {
	opacity:0;
	visibility:hidden;
}
.header__mobile-menu {
	-ms-transition:opacity 0.5s, visibility 0s 0.5s, -ms-transform 0.5s;
	-webkit-transition:opacity 0.5s, visibility 0s 0.5s, -webkit-transform 0.5s;
	transition:opacity 0.5s, visibility 0s 0.5s, transform 0.5s;
}
.flexslider-container,
#slideshow-sale-tag {
	-ms-transition:opacity 0.5s 0.5s, visibility 0s;
	-webkit-transition:opacity 0.5s 0.5s, visibility 0s;
	transition:opacity 0.5s 0.5s, visibility 0s;
}
body.mobile-menu-visible .header__mobile-menu {
	-ms-transition:opacity 0.5s, visibility 0s 0s, -ms-transform 0.5s;
	-webkit-transition:opacity 0.5s, visibility 0s 0s, -webkit-transform 0.5s;
	transition:opacity 0.5s, visibility 0s 0s, transform 0.5s;
}
body.template--index.mobile-menu-visible .header__mobile-menu {
	-ms-transition:opacity 0.5s 0.5s, visibility 0s 0.1s, -ms-transform 0.5s 0.5s;
	-webkit-transition:opacity 0.5s 0.5s, visibility 0s 0.1s, -webkit-transform 0.5s 0.5s;
	transition:opacity 0.5s 0.5s, visibility 0s 0.1s, transform 0.5s 0.5s;
}
body.mobile-menu-visible .flexslider-container,
body.mobile-menu-visible #slideshow-sale-tag {
	-ms-transition:opacity 0.5s, visibility 0s 0.5s;
	-webkit-transition:opacity 0.5s, visibility 0s 0.5s;
	transition:opacity 0.5s, visibility 0s 0.5s;
}

